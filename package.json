{
  "name": "table-editor",
  "version": "1.0.1",
  "description": "",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf docs/",
    "symlink": "(cd docs && ln -s . table-editor)",
    "dupindex": "cp docs/index.html docs/404.html",
    "build": "npm run clean && BUILD=production node_modules/.bin/webpack --bail && npm run dupindex && npm run symlink",
    "devbuild": "npm run clean && BUILD=lproduction node_modules/.bin/webpack --bail && npm run dupindex && npm run symlink && npm run server",
    "dev": "npm run clean && BUILD=development webpack-dev-server --port 8085 --devtool=source-map --progress --colors --content-base app",
    "server": "node_modules/.bin/http-server --p 8085 --cors -c-1 docs/",
    "test": "export BUILD=test && karma start"
  },
  "keywords": [
    "angular",
    "csv",
    "yaml",
    "table",
    "webpack",
    "es6"
  ],
  "author": "Daniel Keith <keitda@ohsu.edu> (http://doctorbud.github.io)",
  "license": "ISC",
